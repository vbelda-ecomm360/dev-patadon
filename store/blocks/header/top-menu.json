{
    "vtex.menu@2.x:menu#top-bar-menu": {
        "children": [
            "menu-item#venta-tele",
            "menu-item#tiendas",
            "menu-item#marcas",
            "menu-item#contacto",
            "menu-item#about-us",
            "menu-item#blog",
            "link#whatsapp",
            "flex-layout.row#wrapper-tel"
        ],
        "props": {
            "blockClass": "top-bar-menu-container"
        }
    },
    "menu-item#marcas": {
        "props": {
          "id": "menu-item-marcas",
          "type": "custom",
          "iconId": null,
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/marcas-de-golf",
            "noFollow": true,
            "tagTitle": "Marcas",
            "text": "Marcas"
          }
        },
        "blocks": [
          "vtex.menu@2.x:submenu#marcas"
        ]
      },
      "vtex.menu@2.x:submenu#marcas": {
        "props": {
          "width": "100%",
          "blockClass": "header-submenus"
          
        },
        "children": [      
          "flex-layout.row#home-brands"
        ]
      },
    "menu-item#venta-tele": {
        "props": {
            "id": "menu-item-venta-tele",
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "itemProps": {
                "type": "internal",
                "href": "/nosotros-te-llamamos",
                "noFollow": false,
                "tagTitle": "Venta Telefónica",
                "text": "Venta Telefónica"
            }
        }
    },
    "menu-item#tiendas": {
        "props": {
            "id": "menu-item-tiendas",
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "itemProps": {
                "type": "internal",
                "href": "/tiendas",
                "noFollow": false,
                "tagTitle": "Nuestras Tiendas",
                "text": "Nuestras Tiendas"
            }
        }
    },
    "menu-item#contacto": {
        "props": {
            "blockClass": "top-bar-menu-contacto",
            "id": "menu-item-contacto",
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "itemProps": {
                "type": "internal",
                "href": "/contacto",
                "noFollow": false,
                "tagTitle": "Formulario de Contacto",
                "text": "Formulario de Contacto"
            }
        }
    },
    "menu-item#about-us": {
        "props": {
            "blockClass": "top-bar-menu-aboutus",
            "id": "menu-item-about-us",
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "itemProps": {
                "type": "internal",
                "href": "/about-us",
                "noFollow": false,
                "tagTitle": "Sobre Depique",
                "text": "Sobre Depique"
            }
        }
    },
    "menu-item#blog": {
        "props": {
            "blockClass": "top-bar-menu-blog",
            "id": "menu-item-blog",
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "itemProps": {
                "type": "external",
                "href": "https://depiqueblog.com/",
                "noFollow": false,
                "tagTitle": "Blog",
                "text": "Blog"
            }
        }
    },
    "link#whatsapp": {
        "props": {
            "href": "https://api.whatsapp.com/send?phone=tel:+66645500705",
            "target":"_blank",
            "blockClass": "phonecall",  
            "label": "whatsapp"
        }
    },
    "flex-layout.row#wrapper-tel": {
        "children": [
          "flex-layout.col#telefonofijo"
        ],
        "props": {
            "blockClass": "wrapperTelFijo"
        }
    },
    "flex-layout.col#telefonofijo": {
        "children": [
            "image#telefono",
            "rich-text#contacto"
        ],
        "props": {
          "blockClass": "textColumn",
          "preventVerticalStretch": true
        }
    },
    "image#telefono": {
        "props": {
            "src": "/arquivos/tel.png",
            "maxHeight": 50,
            "maxWidth": 50
        }
    },
    "rich-text#contacto": {
        "props": {
        "text":
        "93 414 52 35",
        "blockClass": "telefonoFijo"
        }
    }
}