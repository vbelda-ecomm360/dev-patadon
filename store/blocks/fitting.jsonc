// NUESTRO PROCESO
  "rich-text#nuestro-proceso": {
    "props": {
      "text": "### ¿Qué puedes esperar del proceso de fitting?",
      "blockClass": "title-fitting"
    }
  },
  "flex-layout.row#nuestro-proceso": {
    "children": [
      "flex-layout.col#nuestro-proceso-1",
      "flex-layout.col#nuestro-proceso-2"
